{
  "name": "@trincadev/driver.js",
  "license": "MIT",
  "private": false,
  "version": "1.3.1.2-trincadev",
  "main": "./dist/driver.js.cjs",
  "module": "./dist/driver.js.mjs",
  "types": "./dist/driver.js.d.ts",
  "homepage": "https://driverjs.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trincadev/driver.js.git"
  },
  "author": "Kamran Ahmed <kamranahmed.se@gmail.com>",
  "contributors": [
    "Alessandro Trinca Tornidor <alessandro@trinca.tornidor.com>"
  ],
  "bugs": {
    "url": "https://github.com/trincadev/driver.js/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/driver.js.d.ts",
      "require": "./dist/driver.js.cjs",
      "import": "./dist/driver.js.mjs"
    },
    "./dist/driver.css": {
      "import": "./dist/driver.css",
      "require": "./dist/driver.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "format": "prettier . --write"
  },
  "files": [
    "!tests/**/*",
    "!docs/**/*",
    "dist/**/*",
    "!dist/**/*.js.map"
  ],
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^3.0.3",
    "dts-bundle-generator": "^9.5.1",
    "postcss": "^8.5.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.3"
  },
  "keywords": [
    "driver.js",
    "driver",
    "tour",
    "guide",
    "overlay",
    "tooltip",
    "walkthrough",
    "product tour",
    "product walkthrough",
    "product guide",
    "product tutorial",
    "product demo",
    "modal",
    "lightbox"
  ]
}
